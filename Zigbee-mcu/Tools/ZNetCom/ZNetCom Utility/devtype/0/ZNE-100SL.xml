<?xml version="1.0" encoding="windows-1252" ?>
<version value="1" />
<devtype value="36" />
<command resetdev="1" download="0" updatefirm="1" test="1" />
<notsamevalue>
	<items value="WEB_PORT|CMD_PORT|C1_PORT" />
</notsamevalue>
<root name="基本信息">
	<item id="1" name="设备型号" readonly="1" cmd="" defaultval="ZNE-100SL" comment="" />
	<item id="2" name="设备固件版本" readonly="1" cmd="VERSION" />
	<item id="3" name="设备名称" readonly="0" cmd="NAME" />
</root>
<root name="密码操作">
	<item id="4" name="当前密码" readonly="2" cmd="" />
	<item id="5" name="是否更改密码" readonly="2" cmd="" defaultval="0">
		<option name="否" value="0" disableitem="6|7" />
		<option name="是" value="1" enableitem="6|7" />
	</item>
	<item id="6" name="新密码" readonly="2" cmd="PASS" />
	<item id="7" name="确认新密码" readonly="2" cmd="" />
</root>
<root name="IP地址信息">
	<item id="8" name="设备IP" readonly="0" cmd="IP" />
	<item id="9" name="设备子网掩码" readonly="0" cmd="MASK" />
	<item id="10" name="设备网关IP" readonly="0" cmd="GATEWAY" />
	<item id="11" name="设备MAC地址" readonly="1" cmd="MAC" />
	<item id="12" name="获取IP方式" readonly="0" cmd="IP_MODE">
		<option name="动态获取" value="0" disableitem="8|9|10" />
		<option name="静态获取" value="1" enableitem="8|9|10" />
	</item>
</root>
<root name="网络参数">
	<item id="13" name="DNS服务器" readonly="0" cmd="DNS" />
	<item id="14" name="网页端口" readonly="0" cmd="WEB_PORT" />
	<item id="15" name="配置端口（命令端口）" readonly="0" cmd="CMD_PORT" />
</root>

<root name="串口%d" repeat="1" startind="1">
	<item id="51" name="设置所有串口配置和此串口相同" readonly="0" cmd="%d_USESAMECFG"  button="1" startid="24" comment="其他串口除了工作端口之外的所有参数将被设置成和此串口一样" />
	
	<item id="16" name="工作方式" readonly="0" cmd="C%d_OP">
		<option name="TCP Server" value="0" disableitem="33|34" />
		<option name="TCP Client" value="1" enableitem="33|34" />
		<option name="UDP" value="3" enableitem="33|34" />
		<option name="禁用此串口" value="4" />
	</item>
	<item id="17" name="工作端口（连接端口）" readonly="0" cmd="C%d_PORT" />
	<item id="18" name="超时断开时间(10ms)" readonly="0" cmd="C%d_IT" comment="单位10毫秒，指定设备超时断开时间, 范围0到60000" />
	<item id="19" name="心跳检测时间(s)" readonly="0" cmd="C%d_TCPAT" comment="在指定时间内发送“心跳检测包”来测试连接是否存在，不存在则自动断开连接，范围0到60000秒" />
	<item id="20" name="帧起始字节(HEX)" readonly="0" cmd="C%d_D1" showhex="1" unuseval="invalid" comment="打勾表示启用，否则为禁用。" />
	<item id="21" name="帧结束字节(HEX)" readonly="0" cmd="C%d_D2" showhex="1" unuseval="invalid" comment="打勾表示启用，否则为禁用。" />

	<item id="22" name="串口波特率" readonly="0" cmd="C%d_BAUD">
		<option name="300" value="300" />
		<option name="600" value="600" />
		<option name="1200" value="1200" />
		<option name="2400" value="2400" />
		<option name="4800" value="4800" />
		<option name="9600" value="9600" />
		<option name="19200" value="19200" />
		<option name="38400" value="38400" />
		<option name="57600" value="57600" />
		<option name="115200" value="115200" />
	</item>
	<item id="23" name="串口数据位" readonly="0" cmd="C%d_DATAB">
		<option name="5" value="5" />
		<option name="6" value="6" />
		<option name="7" value="7" />
		<option name="8" value="8" />
	</item>
	<item id="24" name="串口停止位" readonly="0" cmd="C%d_STOPB">
		<option name="1" value="1" />
		<option name="2(数据位为5时，此项表示1.5个停止位)" value="2" />
	</item>
	<item id="25" name="串口校验位" readonly="0" cmd="C%d_PARITY">
		<option name="无" value="0" />
		<option name="奇校验" value="1" />
		<option name="偶校验" value="2" />
		<option name="强制为1" value="3" />
		<option name="强制为0" value="4" />
	</item>
	<item id="26" name="分包长度" readonly="0" cmd="C%d_SER_LEN" comment="连续接收时分包长度，最大1478字节" />
	<item id="27" name="串口帧间隔(ms)" readonly="0" cmd="C%d_SER_T" comment="最大5000ms" />
	<item id="28" name="清空串口Buffer" readonly="0" cmd="C%d_BUF_CLS">
		<option name="从不清空" value="0" />
		<option name="TCP连接时清空" value="1" />
	</item>
	<item id="29" name="TCP Turbo" readonly="0" cmd="C%d_TCP_TURBO" comment="启用此选项可加速传输速度，不过有可能会影响串口数据分帧。">
		<option name="禁用" value="0" />
		<option name="启用" value="1" />
	</item>
	<item id="30" name="TCP连接断开" readonly="0" cmd="C%d_TCP_CLS">
		<option name="不断开" value="0" />
		<option name="硬件连接断开则断开" value="1" />
	</item>
	<item id="31" name="TCP连接条件" readonly="0" cmd="C%d_LINK_T" comment="只用于TCP CLIENT，可选择接收到串口数据才开始连接，或DTR变化了才连接或上电就连">
		<option name="无条件,上电就连" value="0" />
		<option name="串口接收到任何数据" value="1" />
		<option name="DSR On/DSR Off" value="2" />
	</item>
	<item id="32" name="TCP连接数" readonly="0" cmd="C%d_LINK_NUM" comment="TCP模式下的连接数量。默认1，范围0～1" />
	<item id="33" name="目标端口1（对方端口）" readonly="0" cmd="C%d_CLI_PP1" />
	<item id="34" name="目标IP1（对方IP）" readonly="0" cmd="C%d_CLI_IP1" comment="目标IP可以是IP地址，如192.168.0.1；或IP地址段(只适用于UDP方式)，如192.168.0.1-192.168.0.7；或域名，如www.embedcontrol.com" />
</root>

<root name="设备IO设置">
	<item id="35" name="IO功能设置(Hex)" readonly="0" cmd="IOSTASETTOE" comment="IO功能设置(Hex)" />
</root>

